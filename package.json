{
  "name": "xt-site",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:test": "vite build --mode test",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue,.ts src",
    "lint:fix": "eslint src/**/*.{js,ts,vue} --fix",
    "stylelint": "npx stylelint --aei .vue src",
    "stylint:fix": "npx stylelint ./src/**/*.vue --fix",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@better-scroll/core": "^2.4.2",
    "@element-plus/icons-vue": "^2.0.1",
    "@vueuse/core": "^8.3.1",
    "@vueuse/integrations": "^8.3.1",
    "axios": "^0.26.1",
    "default-passive-events": "^2.0.0",
    "element-plus": "^2.2.2",
    "fuse.js": "^6.6.0",
    "hotkeys-js": "^3.9.3",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "pinia": "^2.0.14",
    "pinia-plugin-persist": "^1.0.0",
    "vue": "^3.2.25",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@iconify-json/ep": "^1.1.3",
    "@iconify-json/ri": "^1.1.1",
    "@iconify/vue": "^3.2.1",
    "@types/mockjs": "^1.0.6",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vitejs/plugin-vue": "^2.3.0",
    "@vue/compiler-sfc": "^3.2.32",
    "@windicss/plugin-animations": "^1.0.9",
    "consola": "^2.15.3",
    "eslint": "^8.13.0",
    "eslint-plugin-vue": "^8.6.0",
    "fast-glob": "^3.2.11",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "sass": "^1.50.0",
    "stylelint": "^14.6.1",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-scss": "^4.2.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.8.6",
    "unplugin-icons": "^0.14.1",
    "unplugin-vue-components": "^0.19.2",
    "vite": "^2.9.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pwa": "^0.11.13",
    "vite-plugin-restart": "^0.1.1",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-plugin-windicss": "^1.8.3",
    "vue-tsc": "^0.29.8",
    "windicss": "^3.5.1",
    "workbox-window": "^6.5.3"
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": [
      "eslint --fix"
    ],
    "*.vue": [
      "stylelint --fix  --custom-syntax postcss-html"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "change-case"
      ]
    }
  }
}
